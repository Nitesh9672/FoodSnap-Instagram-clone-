<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="../styles/style.css" />
    <link rel="stylesheet" href="../styles/form-card.css" />
    <link rel="stylesheet" href="../styles/post-card.css" />
    <link rel="stylesheet" href="../styles/navbar.css" />
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
  </head>
  <body>

    <div class="main">

      <nav class="nav_links">
        <div class="nav-link-inner">
          <div class="brandName">
            <h1>Food Gallery</h1>
            <!-- <img src="" alt="logo" /> -->
          </div>
          <ul class="link-list">
            <li class="list-item">
              <a href="/"><svg
                  class="menu-icon"
                  xmlns="http://www.w3.org/2000/svg"
                  x="0px"
                  y="0px"
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                >
                  <path
                    d="M 12 2 A 1 1 0 0 0 11.289062 2.296875 L 1.203125 11.097656 A 0.5 0.5 0 0 0 1 11.5 A 0.5 0.5 0 0 0 1.5 12 L 4 12 L 4 20 C 4 20.552 4.448 21 5 21 L 9 21 C 9.552 21 10 20.552 10 20 L 10 14 L 14 14 L 14 20 C 14 20.552 14.448 21 15 21 L 19 21 C 19.552 21 20 20.552 20 20 L 20 12 L 22.5 12 A 0.5 0.5 0 0 0 23 11.5 A 0.5 0.5 0 0 0 22.796875 11.097656 L 12.716797 2.3027344 A 1 1 0 0 0 12.710938 2.296875 A 1 1 0 0 0 12 2 z"
                  ></path>
                </svg>
                <span>Home</span>
              </a>
            </li>

            <li class="list-item">
              <a href="/posts/searchpost"><svg
                  class="menu-icon"
                  xmlns="http://www.w3.org/2000/svg"
                  x="0px"
                  y="0px"
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                >
                  <path
                    d="M 9 2 C 5.1458514 2 2 5.1458514 2 9 C 2 12.854149 5.1458514 16 9 16 C 10.747998 16 12.345009 15.348024 13.574219 14.28125 L 14 14.707031 L 14 16 L 19.585938 21.585938 C 20.137937 22.137937 21.033938 22.137938 21.585938 21.585938 C 22.137938 21.033938 22.137938 20.137938 21.585938 19.585938 L 16 14 L 14.707031 14 L 14.28125 13.574219 C 15.348024 12.345009 16 10.747998 16 9 C 16 5.1458514 12.854149 2 9 2 z M 9 4 C 11.773268 4 14 6.2267316 14 9 C 14 11.773268 11.773268 14 9 14 C 6.2267316 14 4 11.773268 4 9 C 4 6.2267316 6.2267316 4 9 4 z"
                  ></path>
                </svg>
                Search
              </a>
            </li>
            <li class="list-item">
              <a href="/posts/addPost"><img
                  class="menu-icon"
                  src="data:image/png;base64,"
                />Add Post</a>
            </li>
            <li class="list-item">
              <a href="/posts"><img
                  class="menu-icon"
                  src="data:image/png;base64,"
                />Post</a>
            </li>
            <li class="list-item">
              <a href="#"><img
                  class="menu-icon"
                  src="data:image/png;base64,"
                />Notification</a>
            </li>
          </ul>
        </div>
      </nav>
      <div class="middle">
        <div class="main-content-inner">
          {{#each data as |d|}}
            <div class="postCard">
              <div class="postCard-inner">
                <div class="user-detail">
                  <div class="user-profile-container">
                    <img
                      src="data:image/png;base64,"
                      alt=""
                      class="user-profile"
                    />
                  </div>
                  <h3 class="user-name">{{d.user.username}}</h3>

                </div>
                <div class="post-title">
                  <h2>{{d.title}}</h2>
                </div>
                <div class="post-image">
                  <img src="{{d.imageURL}}" alt="" />
                </div>
                <div class="user-interaction">
                  <div class="likes">

                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="16"
                      height="16"
                      fill="currentColor"
                      class="bi bi-heart-fill heart-icon likes-interaction"
                      viewBox="0 0 16 16"
                      id={{d._id}}
                    >
                      <path
                        fill-rule="evenodd"
                        d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314z"
                      />
                    </svg>
                  </div>

                  <div class="comment-interaction">
                    <a href="/posts/showComment?id={{d._id}}"><img
                        class="myicon"
                        src="/utilities/photos/comment-png.png"
                        alt="comment"
                      /></a>

                  </div>
                </div>
                <div class="likesCount">{{d.likesCount}} Likes</div>
                <div class="post-caption">
                  <h4>{{d.caption}}</h4>
                </div>
                {{#if d.commentCount}}
                  <div class="comment-count"><a
                      href="/posts/showComment?id={{d._id}}"
                    ><p>View all
                        {{d.commentCount}}
                        Comments</p></a></div>
                {{/if}}
                <button class="deletePost" id={{d._id}}>Delete</button>
                <a
                  class="updatePost"
                  id={{d._id}}
                  href="/posts/updatePost?id={{d._id}}"
                >Update</a>

                <div class="comment-form-card">
                  <form action="/posts/addComment" method="post">
                    <input
                      type="text"
                      class="hidden"
                      value="{{d._id}}"
                      name="post_id"
                    />
                    <div class="flex-row">

                      <input
                        type="text"
                        name="comment"
                        id="comment"
                        class="comment"
                        placeholder="Add a comment..."
                      />
                      <button type="submit">Post</button>
                    </div>
                  </form>
                </div>

              </div>
            </div>

          {{/each}}

        </div>
      </div>
      <div class="right_sidebar"></div>
    </div>
    <script src="../js/script.js"></script>
  </body>
</html>