      <div class="middle">
        <div class="main-content-inner">
          <div class="form-card">
            <form action="/posts/addPost" method="POST">
              <div>
                <label for="title" class="post-title">Post Title</label>
                <input type="text" id="title" name="title" />
              </div>
              <div>
                <label for="image-url" class="image-url">Enter Image</label>
                <input type="text" id="image-url" name="imageURL" />
              </div>
              <div>
                <label for="caption" class="caption">Caption</label>
                <input type="text" id="caption" name="caption" />
              </div>
              <div>
                <label for="hashtag" class="hashtag">HashTags</label>
                <input type="text" id="hashtag" name="hashtag" />
              </div>
              <button type="submit">Add Post</button>
            </form>
          </div>
        </div>
      </div>
      <div class="right_sidebar"></div>
